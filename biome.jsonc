{
  "$schema": "https://biomejs.dev/schemas/2.1.3/schema.json",
  "extends": ["ultracite"],
  "overrides": [
    {
      "includes": [
        "!**/node_modules/**",
        "!**/public/**",
        "!**/dist/**",
        "!**/.next/**",
        "!**/_generated/**/*",
        "!**/*.gen.*",
        "!**/.git/**",
        "!**/.cache/**",
        "./**/*.{json,jsonc,ts,tsx,js,jsx,html,css,graphql}"
      ]
    }
  ]
}
